let frmJson ={
    {
        type:row,
        col:[
            {
                type: 'text',
                id: 'txFName',
                class: 'form-control',
                label: 'First Name',
                css: 'color:black',
                col: 'col-6',
                validateType: {
                    isValidate: true,
                    rule: {
                        require: true,
                        minLength: 0,
                        isEmail: false,
                        isNumeric: false,
                        message: '{LABEL} is required!'
                    }
                },
            {
                type:Text
            },
            {
                type:Text,
            }
        ]
    }
}


for (let i = 0; i < frmJson.length; i++){
    <div class="row">
        for (let k = 0; k < frmJson[i].type.col){
            <div class="col">
                if(frmJson[i].type.col[k] == 'text'){
                    <div>
                        <input type='tex'></input>
                    </div>
                }
            </div>
        }
    </div>
}